{
  "name": "create-veil-app",
  "version": "0.1.0",
  "description": "Privacy-first project scaffolding CLI for Solana",
  "type": "module",
  "bin": {
    "veil": "./dist/index.js",
    "create-veil-app": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
