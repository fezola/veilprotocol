{
  "name": "@veil-protocol/sdk",
  "version": "0.2.0",
  "description": "Complete privacy infrastructure SDK for Solana - Identity, shielded balances, private voting, stealth multisig, private staking",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./identity": {
      "types": "./dist/identity/index.d.ts",
      "import": "./dist/identity/index.mjs",
      "require": "./dist/identity/index.js"
    },
    "./recovery": {
      "types": "./dist/recovery/index.d.ts",
      "import": "./dist/recovery/index.mjs",
      "require": "./dist/recovery/index.js"
    },
    "./transfer": {
      "types": "./dist/transfer/index.d.ts",
      "import": "./dist/transfer/index.mjs",
      "require": "./dist/transfer/index.js"
    },
    "./shielded": {
      "types": "./dist/shielded/index.d.ts",
      "import": "./dist/shielded/index.mjs",
      "require": "./dist/shielded/index.js"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "import": "./dist/tokens/index.mjs",
      "require": "./dist/tokens/index.js"
    },
    "./dex": {
      "types": "./dist/dex/index.d.ts",
      "import": "./dist/dex/index.mjs",
      "require": "./dist/dex/index.js"
    },
    "./wallet-adapter": {
      "types": "./dist/wallet-adapter/index.d.ts",
      "import": "./dist/wallet-adapter/index.mjs",
      "require": "./dist/wallet-adapter/index.js"
    },
    "./voting": {
      "types": "./dist/voting/index.d.ts",
      "import": "./dist/voting/index.mjs",
      "require": "./dist/voting/index.js"
    },
    "./multisig": {
      "types": "./dist/multisig/index.d.ts",
      "import": "./dist/multisig/index.mjs",
      "require": "./dist/multisig/index.js"
    },
    "./staking": {
      "types": "./dist/staking/index.d.ts",
      "import": "./dist/staking/index.mjs",
      "require": "./dist/staking/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "solana",
    "privacy",
    "zk-proofs",
    "shielded",
    "confidential",
    "wallet",
    "defi"
  ],
  "author": "Veil Protocol",
  "license": "MIT",
  "peerDependencies": {
    "@solana/web3.js": "^1.87.0"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "crypto-js": "^4.2.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/veil-protocol/sdk"
  },
  "files": [
    "dist",
    "README.md"
  ]
}

